   "source": [
    "#NaN값이 있는 행만 출력하기\n",
    "df[df[\"A\"].isnull()]\n",
    "\n",
    "#NaN값 일괄 바꾸기 inplace 옵션 사용가능\n",
    "df.fillna()\n",
    "\n",
    "#최빈값 구하기\n",
    "df.mode()\n",
    "\n",
    "#조건에 맞춰 특정 컬럼에 있는 값을 변경하고자 할 경우\n",
    "df.loc[(df.col == 1),\"col\"] = 10 \n",
    "\n",
    "#loc에서 다중 조건으로 필터링하여 값 변경하기\n",
    "df.loc[(df.col >= 1)&(df.col < 10),\"col\"] = 10"
   ]
  }
